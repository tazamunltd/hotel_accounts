<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="ir_cron_generate_manual_postings" model="ir.cron">
        <field name="name">Generate Manual Postings</field>
        <field name="model_id" ref="tz_front_desk.model_tz_manual_posting_type"/>
        <field name="state">code</field>
        <field name="code">model.generate_manual_postings()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>
