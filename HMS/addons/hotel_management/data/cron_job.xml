<odoo>
    <record id="ir_cron_room_results" model="ir.cron">
        <field name="name">Generate Room Results</field>
        <field name="model_id" ref="model_hotel_room_result"/>
        <field name="state">code</field>
        <field name="code">model.generate_room_results()</field>
        <field name="interval_number">3</field>
        <field name="interval_type">minutes</field> <!-- Runs every 1 minute -->
        <field name="numbercall">-1</field> <!-- Infinite execution -->
        <field name="active">True</field>
    </record>
</odoo>